{
  "name": "@agenticflow/editor",
  "version": "0.1.0",
  "description": "ReactFlow-based visual workflow editor for AgenticFlow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/**/*"],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@agenticflow/types": "workspace:*",
    "@agenticflow/core": "workspace:*",
    "@agenticflow/config": "workspace:*",
    "@agenticflow/nodes": "workspace:*",
    "reactflow": "^11.11.4",
    "react-mentions": "^4.4.10",
    "zustand": "^5.0.5"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "typescript": "^5.7.2",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "reactflow",
    "visual-editor",
    "workflow",
    "automation",
    "agenticflow"
  ],
  "author": "AgenticFlow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agenticflow/agenticflow.git",
    "directory": "packages/editor"
  }
} 